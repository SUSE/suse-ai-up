{
  "name": "virtualmcp-server",
  "version": "1.0.0",
  "description": "VirtualMCP server template for running MCP implementations as HTTP servers",
  "main": "virtualmcp-server.ts",
  "scripts": {
    "start": "tsx virtualmcp-server.ts",
    "dev": "tsx --watch virtualmcp-server.ts",
    "build": "tsc virtualmcp-server.ts --outDir dist --target es2020 --module commonjs",
    "type-check": "tsc --noEmit virtualmcp-server.ts",
    "http": "tsx virtualmcp-server.ts --transport http",
    "http-dev": "tsx --watch virtualmcp-server.ts --transport http"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/cors": "^2.8.0",
    "@types/uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "server",
    "virtualmcp",
    "http",
    "typescript"
  ],
  "author": "SUSE AI Team",
  "license": "Apache-2.0"
}