[
  {
    "id": "suse-trento",
    "name": "SUSE Trento",
    "description": "Official SUSE MCP server for SAP system monitoring and management through Trento Project. Enables AI assistants to monitor cluster health, system configurations, and SAP landscapes through natural language queries.",
    "version": "1.0.0",
    "packages": [
      {
        "registryType": "remote-http",
        "identifier": "trento-server",
        "transport": {
          "type": "http"
        },
        "environmentVariables": [
          {
            "name": "TRENTO_CONFIG",
            "description": "Trento configuration in format: TRENTO_URL={url},TOKEN={pat}",
            "format": "string",
            "isSecret": true,
            "default": ""
          }
        ]
      },
      {
        "registryType": "stdio",
        "identifier": "mcp-server-trento",
        "transport": {
          "type": "stdio"
        },
        "environmentVariables": [
          {
            "name": "TRENTO_URL",
            "description": "Target Trento server URL (e.g., https://trento.example.com)",
            "format": "string",
            "isSecret": false,
            "default": ""
          },
          {
            "name": "TRENTO_PAT",
            "description": "Trento Personal Access Token for authentication",
            "format": "string",
            "isSecret": true,
            "default": ""
          }
        ]
      }
    ],
    "validationStatus": "approved",
    "discoveredAt": "2025-01-01T00:00:00Z",
    "_meta": {
      "source": "suse",
      "badges": ["SUSE", "CERTIFIED"],
      "priority": 200,
      "logo": "suse-logo",
      "featured": true,
      "category": "sap-management",
      "tags": ["trento", "sap", "hana", "cluster-monitoring", "system-management", "suse"],
      "hosted": false,
      "transportType": "remote-http",
      "deploymentOptions": ["helm", "docker", "systemd", "local"],
      "helmChart": "oci://registry.suse.com/trento/trento-server",
      "helmCommand": "helm upgrade --install trento-server oci://registry.suse.com/trento/trento-server --set trento-mcp-server.enabled=true --set trento-mcp-server.mcpServer.trentoURL={TRENTO_URL}",
      "documentation": "https://github.com/trento-project/mcp-server",
      "setupInstructions": "Requires Trento server 3.x+ and Personal Access Token. Supports both remote HTTP and local stdio deployment modes."
    }
  },
